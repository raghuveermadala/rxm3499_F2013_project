import sys
import simplejson


def getEmoticonsTweets(argfile):
    fil = open(argfile,"r")
    outfile = open("nyc.trim.emoticons", "w")
    
    all_lines = fil.readlines()
     
    for line in all_lines:
	
        try:
            tweet = simplejson.loads(line)
            if 'doc' in tweet:
                doc = tweet["doc"]
                text_=doc["from_user"]
		print text_
        except ValueError:
        pass
        print "Error Occured"

fil.close()
outfile.close()    

def main():
    getEmoticonsTweets(sys.argv[1])

if __name__ == '__main__':
    main()   
